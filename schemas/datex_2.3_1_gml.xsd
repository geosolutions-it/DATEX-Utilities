                            <xs:documentation>The vertical height measured in metres of the specific
                                pictogram display area.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="pictogramDisplayWidth" type="npra:MetresAsFloat">
