                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="speedManagementExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element maxOccurs="unbounded" minOccurs="1" name="reroutingManagementType" type="npra:ReroutingManagementTypeEnum">
