                    <xs:element minOccurs="0" name="nonOrderedLocationGroupByReferenceExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element minOccurs="0" name="nonOrderedLocationsExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element minOccurs="0" name="groupOfLocationsExtension" type="npra:_ExtensionTypeType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="NonOrderedLocationGroupByReferencePropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="npra:NonOrderedLocationGroupByReference"/>
        </xs:sequence>
        <xs:attributeGroup ref="gml:AssociationAttributeGroup"/>
    </xs:complexType>
    <xs:element name="OperatorAction" substitutionGroup="gml:AbstractFeature" type="npra:OperatorActionType"/>
    <xs:complexType name="OperatorActionType">
        <xs:complexContent>
            <xs:extension base="gml:AbstractFeatureType">
                <xs:annotation>
                    <xs:documentation>Actions that a traffic operator can decide to implement to
                        prevent or help correct dangerous or poor driving conditions, including
                        maintenance of the road infrastructure.</xs:documentation>
                    <xs:documentation>Speed management action that is instigated by the network/road
                        operator.</xs:documentation>
                    <xs:documentation>Rerouting management action that is issued by the network/road
                        operator.</xs:documentation>
                    <xs:documentation>Road, carriageway or lane management action that is instigated
                        by the network/road operator.</xs:documentation>
                    <xs:documentation>Network management action that is instigated either manually
                        or automatically by the network/road operator. Compliance with any resulting
                        control may be advisory or mandatory.</xs:documentation>
                    <xs:documentation>Winter driving management action that is instigated by the
                        network/road operator.</xs:documentation>
                    <xs:documentation>Provides information on message signs and the information
                        currently displayed.</xs:documentation>
                    <xs:documentation>An identifiable versioned instance of a single record/element
                        within a situation.</xs:documentation>
                    <xs:documentation>Highway maintenance, installation and construction activities
                        that may potentially affect traffic operations.</xs:documentation>
                    <xs:documentation>Network management action which is applicable to the road
                        network and its users.</xs:documentation>
                    <xs:documentation>Roadworks involving the maintenance or installation of
                        infrastructure.</xs:documentation>
                    <xs:documentation>General instruction and/or message that is issued by the
                        network/road operator which is applicable to drivers and sometimes
                        passengers.</xs:documentation>
                    <xs:documentation>Roadworks involving the construction of new
                        infrastructure.</xs:documentation>
                    <xs:documentation>Details of road side assistance required or being
                        given.</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="actionOrigin" type="npra:OperatorActionOriginEnum">
                        <xs:annotation>
                            <xs:documentation>Indicates whether the actions to be undertaken by the
                                operator are the result of an internal operation or external
                                influence.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="actionPlanIdentifier" type="npra:String">
                        <xs:annotation>
                            <xs:documentation>The identifier of the traffic management action plan
                                to which this action relates.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="operatorActionStatus" type="npra:OperatorActionStatusEnum">
                        <xs:annotation>
                            <xs:documentation>The status of the defined operator
