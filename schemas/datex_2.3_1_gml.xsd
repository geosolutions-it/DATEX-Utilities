                    <xs:element minOccurs="0" name="humidity" type="npra:HumidityType"/>
                    <xs:element minOccurs="0" name="poorEnvironmentConditionsExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="constructionWorkType" type="npra:ConstructionWorkTypeEnum">
                        <xs:annotation>
                            <xs:documentation>The type of construction work being
                                performed.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="constructionWorksExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="journeyDestination" type="npra:MultilingualStringType">
