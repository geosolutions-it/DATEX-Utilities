                    <xs:element minOccurs="0" name="openlrBasePointLocationExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element minOccurs="0" name="openlrPointAlongLineExtension" type="npra:_ExtensionTypeType"/>
