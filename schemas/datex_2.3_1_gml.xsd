                    <xs:element maxOccurs="1" minOccurs="0" name="cctvVideoBitRate" type="npra:NonNegativeInteger">
                        <xs:annotation>
                            <xs:documentation>The bit rate of this video service level measured in
                                bits per second.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="cctvVideoZoomBlankingLevel" type="npra:NonNegativeInteger">
