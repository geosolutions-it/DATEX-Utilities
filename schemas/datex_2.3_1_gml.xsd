    <xs:element name="OpenlrAreaLocationReference" substitutionGroup="gml:AbstractFeature" type="npra:OpenlrAreaLocationReferenceType"/>
    <xs:complexType name="OpenlrAreaLocationReferenceType">
        <xs:complexContent>
            <xs:extension base="gml:AbstractFeatureType">
                <xs:annotation>
                    <xs:documentation>a two-dimensional part of the surface of the earth which is
                        bounded by a closed curve. An area location may cover parts of the road
                        network but does not necessarily need to. It is represente according to the
                        OpenLR standard for Area Locations</xs:documentation>
                    <xs:documentation>the openLR method of areadefinition by providing repeating
                        rectangles</xs:documentation>
                    <xs:documentation>the openLR method of areadefinition by providing points that
                        bound the area</xs:documentation>
                    <xs:documentation>the openLR method of areadefinition by providing a closed path
                        (i.e. a circuit) in the road network. The boundary always consists of road
                        segments</xs:documentation>
                    <xs:documentation>the openLR method of areadefinition by providing a center
                        position and a radius</xs:documentation>
                    <xs:documentation>the openLR method of areadefinition by providing a rectangular
                        shape defined by two geo-coordinate pairs</xs:documentation>
                </xs:annotation>
                <xs:sequence>
                    <xs:element minOccurs="0" name="openlrAreaLocationReferenceExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element name="openlrRectangle" type="npra:OpenlrRectangleType"/>
                    <xs:element name="openlrGridAttributes" type="npra:OpenlrGridAttributesType"/>
                    <xs:element minOccurs="0" name="openlrGridLocationReferenceExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element name="openlrPolygonCorners" type="npra:OpenlrPolygonCornersType"/>
                    <xs:element minOccurs="0" name="openlrPolygonLocationReferenceExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element maxOccurs="unbounded" name="openlrLocationReferencePoint" type="npra:OpenlrLocationReferencePointPropertyType"/>
                    <xs:element name="openlrLastLine" type="npra:OpenlrLineAttributesType"/>
                    <xs:element minOccurs="0" name="openlrClosedLineLocationReferenceExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element maxOccurs="1" minOccurs="1" name="radius" type="npra:MetresAsNonNegativeInteger">
                        <xs:annotation>
                            <xs:documentation>The radius of the geometric area
                                identified.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="openlrGeoCoordinate" type="npra:OpenlrGeoCoordinateType"/>
                    <xs:element minOccurs="0" name="openlrCircleLocationReferenceExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element name="openlrRectangle" type="npra:OpenlrRectangleType"/>
                    <xs:element minOccurs="0" name="openlrRectangleLocationReferenceExtension" type="npra:_ExtensionTypeType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="OpenlrAreaLocationReferencePropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="npra:OpenlrAreaLocationReference"/>
        </xs:sequence>
