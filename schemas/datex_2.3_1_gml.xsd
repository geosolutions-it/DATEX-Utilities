                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="distanceFromLinearElementReferentExtension" type="npra:_ExtensionTypeType"/>
                    <xs:element maxOccurs="1" minOccurs="1" name="distanceAlong" type="npra:MetresAsFloat">
